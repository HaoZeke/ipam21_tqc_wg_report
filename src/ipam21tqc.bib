@article{robertsImplementationRigorousRenormalization2017,
  title = {Implementation of Rigorous Renormalization Group Method for Ground Space and Low-Energy States of Local {{Hamiltonians}}},
  author = {Roberts, Brenden and Vidick, Thomas and Motrunich, Olexei I.},
  date = {2017-12-18},
  journaltitle = {Physical Review B},
