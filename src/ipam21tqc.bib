@article{khoromskaiaBlackBoxHartreeFock2014a,
  title = {Black-{{Box Hartree}}–{{Fock Solver}} by {{Tensor Numerical Methods}}},
  author = {Khoromskaia, Venera},
  date = {2014-01-01},
  journaltitle = {Computational Methods in Applied Mathematics},
  volume = {14},
  pages = {89--111},
  publisher = {{Walter de Gruyter GmbH}},
  issn = {1609-9389},
  doi = {10.1515/cmam-2013-0023},
  url = {https://www.degruyter.com/document/doi/10.1515/cmam-2013-0023/html},
  urldate = {2021-06-06},
  abstract = {Abstract. The Hartree–Fock eigenvalue problem governed by the 3D integro-differential operator is the basic model in ab initio electronic structure calculations. Several years ago the idea to solve the Hartree–Fock equation by a fully 3D grid based numerical approach seemed to be a fantasy, and the tensor-structured methods did not exist. In fact, these methods evolved during the work on this challenging problem. In this paper, our recent results on the topic are outlined and the black-box Hartree–Fock solver by tensor numerical methods is presented. The approach is based on the rank-structured calculation of the core hamiltonian and of the two-electron integrals tensor using the problem adapted basis functions discretized on n×n×n\$n\textbackslash times n\textbackslash times n\$ 3D Cartesian grids. The arising 3D convolution transforms with the Newton kernel are replaced by a combination of 1D convolutions and 1D Hadamard and scalar products. The approach allows huge spatial grids, with n3≃1015\$n\^3\textbackslash simeq 10\^\{15\}\$, yielding high resolution at low cost. The two-electron integrals are computed via multiple factorizations. The Laplacian Galerkin matrix can be computed “on-the-fly” using the quantized tensor approximation of O(logn)\$O(\textbackslash log n)\$ complexity. The performance of the black-box solver in Matlab implementation is compatible with the benchmark packages based on the analytical (pre)evaluation of the multidimensional convolution integrals. We present ab initio Hartree–Fock calculations of the ground state energy for the amino acid molecules, and of the “energy bands” for the model examples of extended (quasi-periodic) systems.},
  langid = {english},
  number = {1}
}

@article{khoromskaiaQTTRepresentationHartree2011,
  title = {{{QTT Representation}} of the {{Hartree}} and {{Exchange Operators}} in {{Electronic Structure Calculations}}},
  author = {Khoromskaia, Venera and Khoromskij, Boris and Schneider, Reinhold},
  date = {2011-01-01},
  journaltitle = {Computational Methods in Applied Mathematics},
  volume = {11},
  pages = {327--341},
  publisher = {{De Gruyter}},
  issn = {1609-9389},
  doi = {10.2478/cmam-2011-0018},
  url = {https://www.degruyter.com/document/doi/10.2478/cmam-2011-0018/html},
  urldate = {2021-04-29},
  abstract = {In this paper, the tensor-structured numerical evaluation of the Coulomb and exchange operators in the Hartree-Fock equation is supplemented by the usage of recent quantized-TT (QTT) formats. It leads to O(log n) complexity at computationally extensive stages in the rank-structured calculation with the respective 3D Hartree and exchange potentials discretized on large n×n×n Cartesian grids. The numerical examples for some volumetric organic molecules confirm that the QTT ranks of these potentials are nearly independent of the one-dimension grid size n. Thus, paradoxically, the complexity of the grid-based evaluation of the Coulumb and exchange matrices becomes almost independent of the grid size, being regulated only by the structure of a molecular system. As a result, the grid approximation of the Hartree-Fock equation allows to gain the high resolution with a guaranteed accuracy.},
  langid = {english},
  number = {3}
}

