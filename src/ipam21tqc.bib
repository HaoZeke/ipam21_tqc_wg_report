@article{venderleyDMRGStudyPairdensity2019,
  title = {A {{DMRG}} Study: {{Pair}}-Density Wave in Spin-Valley Locked Systems},
  shorttitle = {A {{DMRG}} Study},
  author = {Venderley, Jordan and Kim, Eun-Ah},
  date = {2019-03},
  journaltitle = {Science Advances},
