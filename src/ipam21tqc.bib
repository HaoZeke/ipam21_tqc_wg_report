@article{wagnerReferenceElectronicStructure2012,
  title = {Reference Electronic Structure Calculations in One Dimension},
  author = {Wagner, Lucas O. and Stoudenmire, E. M. and Burke, Kieron and White, Steven R.},
  date = {2012-05-30},
